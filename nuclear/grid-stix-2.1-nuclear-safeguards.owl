<?xml version="1.0"?>
<rdf:RDF xmlns:owl="http://www.w3.org/2002/07/owl#" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xml:base="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl">
  <owl:Ontology rdf:about="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl">
    <owl:imports rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-assets.owl"/>
    <owl:imports rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-vocab.owl"/>
    <owl:imports rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl"/>
    <owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/observed-data"/>
    <rdfs:label>grid_stix_2_1_nuclear_safeguards</rdfs:label>
    <rdfs:comment>Nuclear safeguards, non-proliferation, and material control extensions for Grid-STIX 2.1</rdfs:comment>
  </owl:Ontology>

  <!-- =============================================== -->
  <!-- NUCLEAR FACILITY AND ASSET CLASSES -->
  <!-- =============================================== -->
  
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#nuclear-facility">
    <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#nuclear-power-plant"/>
    <rdfs:comment>Nuclear facility subject to safeguards oversight and non-proliferation monitoring.</rdfs:comment>
    <rdfs:label>nuclear_facility</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#material-balance-area">
    <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-assets.owl#security-zone"/>
    <rdfs:comment>Defined area within a nuclear facility where nuclear material quantities can be determined.</rdfs:comment>
    <rdfs:label>material_balance_area</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#key-measurement-point">
    <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-assets.owl#physical-asset"/>
    <rdfs:comment>Location where nuclear material quantities are measured to maintain material accountancy.</rdfs:comment>
    <rdfs:label>key_measurement_point</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#nuclear-material">
    <rdfs:subClassOf rdf:resource="http://docs.oasis-open.org/ns/cti/stix/infrastructure"/>
    <rdfs:comment>Nuclear material subject to safeguards monitoring and control.</rdfs:comment>
    <rdfs:label>nuclear_material</rdfs:label>
  </owl:Class>

  <!-- =============================================== -->
  <!-- SAFEGUARDS MONITORING SYSTEMS -->
  <!-- =============================================== -->
  
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#safeguards-system">
    <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-assets.owl#ot-device"/>
    <rdfs:comment>Monitoring system used for nuclear safeguards verification and material control.</rdfs:comment>
    <rdfs:label>safeguards_system</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#containment-surveillance-system">
    <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#safeguards-system"/>
    <rdfs:comment>Surveillance system monitoring containment and surveillance measures.</rdfs:comment>
    <rdfs:label>containment_surveillance_system</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#radiation-detection-system">
    <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#safeguards-system"/>
    <rdfs:comment>System for detecting and measuring radiation signatures from nuclear materials.</rdfs:comment>
    <rdfs:label>radiation_detection_system</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#material-accounting-system">
    <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#safeguards-system"/>
    <rdfs:comment>System for tracking and accounting for nuclear material quantities and locations.</rdfs:comment>
    <rdfs:label>material_accounting_system</rdfs:label>
  </owl:Class>

  <!-- =============================================== -->
  <!-- NUCLEAR SAFEGUARDS PROPERTIES -->
  <!-- =============================================== -->
  
  <owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#facility_type">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#nuclear-facility"/>
    <rdfs:range rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-vocab.owl#nuclear_facility_type_ov"/>
    <rdfs:comment>Type of nuclear facility from controlled vocabulary.</rdfs:comment>
    <rdfs:label>facility_type</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#safeguards_agreement">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#nuclear-facility"/>
    <rdfs:range rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-vocab.owl#safeguards_agreement_type_ov"/>
    <rdfs:comment>Type of safeguards agreement governing the facility.</rdfs:comment>
    <rdfs:label>safeguards_agreement</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#material_type">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#nuclear-material"/>
    <rdfs:range rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-vocab.owl#nuclear_material_type_ov"/>
    <rdfs:comment>Type of nuclear material from controlled vocabulary.</rdfs:comment>
    <rdfs:label>material_type</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#enrichment_level">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#nuclear-material"/>
    <rdfs:range rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-vocab.owl#enrichment_level_ov"/>
    <rdfs:comment>Enrichment level category of fissile material.</rdfs:comment>
    <rdfs:label>enrichment_level</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#detection_method">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#radiation-detection-system"/>
    <rdfs:range rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-vocab.owl#radiation_detection_method_ov"/>
    <rdfs:comment>Method used for radiation detection and measurement.</rdfs:comment>
    <rdfs:label>detection_method</rdfs:label>
  </owl:ObjectProperty>

  <!-- =============================================== -->
  <!-- MATERIAL CONTROL PROPERTIES -->
  <!-- =============================================== -->
  
  <owl:DatatypeProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#material_quantity_kg">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#nuclear-material"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:comment>Quantity of nuclear material in kilograms.</rdfs:comment>
    <rdfs:label>material_quantity_kg</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#u235_content_percent">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#nuclear-material"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:comment>Uranium-235 content as percentage of total uranium.</rdfs:comment>
    <rdfs:label>u235_content_percent</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#measurement_uncertainty">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#key-measurement-point"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:comment>Measurement uncertainty as percentage for material accounting.</rdfs:comment>
    <rdfs:label>measurement_uncertainty</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#inspection_frequency_days">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#nuclear-facility"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:comment>Required inspection frequency in days for safeguards verification.</rdfs:comment>
    <rdfs:label>inspection_frequency_days</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#significant_quantity_threshold">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#material-balance-area"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:comment>Significant quantity threshold for nuclear material in this area.</rdfs:comment>
    <rdfs:label>significant_quantity_threshold</rdfs:label>
  </owl:DatatypeProperty>

  <!-- =============================================== -->
  <!-- SAFEGUARDS RELATIONSHIPS -->
  <!-- =============================================== -->
  
  <owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#monitors_material">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#safeguards-system"/>
    <rdfs:range rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#nuclear-material"/>
    <rdfs:comment>Indicates that a safeguards system monitors specific nuclear material.</rdfs:comment>
    <rdfs:label>monitors_material</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#contains_material">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#material-balance-area"/>
    <rdfs:range rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#nuclear-material"/>
    <rdfs:comment>Indicates that a material balance area contains specific nuclear material.</rdfs:comment>
    <rdfs:label>contains_material</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#measures_at_point">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#material-accounting-system"/>
    <rdfs:range rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#key-measurement-point"/>
    <rdfs:comment>Indicates that an accounting system takes measurements at a specific key measurement point.</rdfs:comment>
    <rdfs:label>measures_at_point</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#provides_containment_for">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#containment-surveillance-system"/>
    <rdfs:range rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#material-balance-area"/>
    <rdfs:comment>Indicates that a containment surveillance system provides containment for a material balance area.</rdfs:comment>
    <rdfs:label>provides_containment_for</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#subject_to_inspection">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#nuclear-facility"/>
    <rdfs:range rdf:resource="http://docs.oasis-open.org/ns/cti/stix/identity"/>
    <rdfs:comment>Indicates that a nuclear facility is subject to inspection by a regulatory authority.</rdfs:comment>
    <rdfs:label>subject_to_inspection</rdfs:label>
  </owl:ObjectProperty>

  <!-- =============================================== -->
  <!-- ANOMALY AND DIVERSION DETECTION -->
  <!-- =============================================== -->
  
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#material-diversion-indicator">
    <rdfs:subClassOf rdf:resource="http://docs.oasis-open.org/ns/cti/stix/indicator"/>
    <rdfs:comment>Indicator pattern suggesting potential diversion or misuse of nuclear material.</rdfs:comment>
    <rdfs:label>material_diversion_indicator</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#safeguards-anomaly">
    <rdfs:subClassOf rdf:resource="http://docs.oasis-open.org/ns/cti/stix/observed-data"/>
    <rdfs:comment>Observed anomaly in safeguards systems or material accounting that may indicate proliferation concern.</rdfs:comment>
    <rdfs:label>safeguards_anomaly</rdfs:label>
  </owl:Class>

  <owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#anomaly_type">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#safeguards-anomaly"/>
    <rdfs:range rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-vocab.owl#safeguards_anomaly_type_ov"/>
    <rdfs:comment>Type of safeguards anomaly from controlled vocabulary.</rdfs:comment>
    <rdfs:label>anomaly_type</rdfs:label>
  </owl:ObjectProperty>

  <owl:DatatypeProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#material_unaccounted_for_kg">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#safeguards-anomaly"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:comment>Quantity of nuclear material unaccounted for in kilograms.</rdfs:comment>
    <rdfs:label>material_unaccounted_for_kg</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#detection_confidence">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#material-diversion-indicator"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:comment>Confidence level for material diversion detection (0.0-1.0).</rdfs:comment>
    <rdfs:label>detection_confidence</rdfs:label>
  </owl:DatatypeProperty>

</rdf:RDF>