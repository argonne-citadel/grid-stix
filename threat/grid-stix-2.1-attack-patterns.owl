  <owl:NamedIndividual rdf:about="http://www.anl.gov/sss/grid-stix-2.1-attack-patterns.owl#dnp3-spoofing-attack">
    <rdf:type rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-attack-patterns.owl#protocol-attack-pattern"/>
    <ns4:name>DNP3 Control Command Spoofing</ns4:name>
    <rdfs:label>dnp3_control_command_spoofing</rdfs:label>
    <ns4:description>Attackers send spoofed DNP3 control commands to RTUs or IEDs to manipulate grid operations.</ns4:description>
    <ns4:affects_protocol rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-vocab.owl#grid-protocol-dnp3"/>
    <ns4:has_impact_type rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-attack-patterns.owl#data-integrity"/>
    <ns4:has_impact_type rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-attack-patterns.owl#outage"/>
    <ns4:severity rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</ns4:severity>
    <ns4:likelihood_of_success rdf:datatype="http://www.w3.org/2001/XMLSchema#float">0.7</ns4:likelihood_of_success>
    <ns4:skill_level_required rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</ns4:skill_level_required>
    <ns4:capec_id>CAPEC-94</ns4:capec_id>
    <ns4:attack_id>T0802</ns4:attack_id>
  </owl:NamedIndividual>
