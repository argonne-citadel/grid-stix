  <owl:Ontology rdf:about="http://www.anl.gov/sss/grid-stix-2.1-assets.owl">
    <owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/infrastructure"/>
    <owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/location"/>
    <owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/software"/>
    <owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/identity"/>
    <owl:imports rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-vocab.owl"/>
    <rdfs:label>grid_stix_2_1_assets</rdfs:label>
    <rdfs:comment>Grid-STIX 2.1: Asset classes extending STIX Infrastructure, Location, Software, and Identity SDOs</rdfs:comment>
  </owl:Ontology>
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-assets.owl#GridComponent">
    <rdfs:subClassOf rdf:resource="http://docs.oasis-open.org/ns/cti/stix#Infrastructure"/>
    <rdfs:comment>A grid component such as circuit breakers, switches, or relays.</rdfs:comment>
    <rdfs:label>grid_component</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-assets.owl#OTDevice">
    <rdfs:subClassOf rdf:resource="http://docs.oasis-open.org/ns/cti/stix#Software"/>
    <rdfs:comment>An OT device's software or firmware component, e.g., PLC or SCADA module.</rdfs:comment>
    <rdfs:label>ot_device</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-assets.owl#PhysicalAsset">
    <rdfs:subClassOf rdf:resource="http://docs.oasis-open.org/ns/cti/stix#Infrastructure"/>
    <rdfs:comment>A physical asset in the distribution grid, e.g., poles, transformers, substations.</rdfs:comment>
    <rdfs:label>physical_asset</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-assets.owl#Substation">
    <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-assets.owl#PhysicalAsset"/>
    <rdfs:comment>A facility that contains equipment for switching and transforming electricity between different voltage levels.</rdfs:comment>
    <rdfs:label>substation</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-assets.owl#Transformer">
    <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-assets.owl#PhysicalAsset"/>
    <rdfs:comment>An electrical device that transfers electrical energy between two or more circuits through electromagnetic induction.</rdfs:comment>
    <rdfs:label>transformer</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-assets.owl#TransmissionLine">
    <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-assets.owl#PhysicalAsset"/>
    <rdfs:comment>A high-voltage power line that transfers bulk electrical energy from generating plants to substations.</rdfs:comment>
    <rdfs:label>transmission_line</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-assets.owl#DistributionLine">
    <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-assets.owl#PhysicalAsset"/>
    <rdfs:comment>A medium or low-voltage power line that carries electricity from substations to consumers.</rdfs:comment>
    <rdfs:label>distribution_line</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-assets.owl#Generator">
    <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-assets.owl#PhysicalAsset"/>
    <rdfs:comment>A device that converts mechanical energy to electrical energy for use in an external circuit.</rdfs:comment>
    <rdfs:label>generator</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-assets.owl#ControlCenter">
    <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-assets.owl#PhysicalAsset"/>
    <rdfs:comment>A facility used to monitor and control the grid, typically housing SCADA systems and other control equipment.</rdfs:comment>
    <rdfs:label>control_center</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-assets.owl#SecurityZone">
    <rdfs:subClassOf rdf:resource="http://docs.oasis-open.org/ns/cti/stix#Location"/>
    <rdfs:comment>A security zone within the grid, e.g., substation perimeter or fenced area.</rdfs:comment>
    <rdfs:label>security_zone</rdfs:label>
  </owl:Class>
