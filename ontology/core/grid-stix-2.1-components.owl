<?xml version="1.0"?>
<rdf:RDF xmlns:owl="http://www.w3.org/2002/07/owl#" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xml:base="http://www.anl.gov/sss/grid-stix-2.1-components.owl">
  <owl:Ontology rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl">
    <owl:imports rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-assets.owl"/>
    <owl:imports rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-vocab.owl"/>
    <rdfs:label>grid_stix_2_1_components</rdfs:label>
    <rdfs:comment>Definitions of grid component classes and their properties, extending the base asset classes</rdfs:comment>
  </owl:Ontology>
  <!-- Generation Asset Hierarchy -->
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#generation-asset">
    <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-assets.owl#grid-component"/>
    <rdfs:comment>Base class for all electricity generation assets and facilities in the power grid.</rdfs:comment>
    <rdfs:label>generation_asset</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#distributed-energy-resource">
    <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#generation-asset"/>
    <rdfs:comment>Small-scale power generation and storage resources connected to the distribution grid.</rdfs:comment>
    <rdfs:label>distributed_energy_resource</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#centralized-generation-facility">
    <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#generation-asset"/>
    <rdfs:comment>Large-scale centralized power generation facilities that supply electricity to the transmission grid.</rdfs:comment>
    <rdfs:label>centralized_generation_facility</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#renewable-generation-facility">
    <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#centralized-generation-facility"/>
    <rdfs:comment>Utility-scale renewable energy generation facilities such as wind farms and solar installations.</rdfs:comment>
    <rdfs:label>renewable_generation_facility</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#fossil-fuel-plant">
    <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#centralized-generation-facility"/>
    <rdfs:comment>Power generation facilities that burn fossil fuels such as coal, natural gas, or oil.</rdfs:comment>
    <rdfs:label>fossil_fuel_plant</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#nuclear-power-plant">
    <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-nuclear-safeguards.owl#nuclear-facility"/>
    <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#centralized-generation-facility"/>
    <rdfs:comment>Nuclear power generation facilities using nuclear fission to produce electricity.</rdfs:comment>
    <rdfs:label>nuclear_power_plant</rdfs:label>
  </owl:Class>
  <!-- Distribution Asset Hierarchy -->
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#distribution-asset">
    <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-assets.owl#grid-component"/>
    <rdfs:comment>Components that distribute electrical energy from transmission systems to end users.</rdfs:comment>
    <rdfs:label>distribution_asset</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#wind-turbine">
    <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#distributed-energy-resource"/>
    <rdfs:comment>Wind-powered generator that converts kinetic wind energy into electrical energy through rotating blades.</rdfs:comment>
    <rdfs:label>wind_turbine</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#fuel-cell">
    <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#distributed-energy-resource"/>
    <rdfs:comment>Electrochemical device that converts hydrogen and oxygen into electricity with water and heat as byproducts.</rdfs:comment>
    <rdfs:label>fuel_cell</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#inverter">
    <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-assets.owl#grid-component"/>
    <rdfs:comment>Power electronic device that converts direct current (DC) to alternating current (AC) for grid integration of DER systems.</rdfs:comment>
    <rdfs:label>inverter</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#microgrid">
    <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-assets.owl#physical-asset"/>
    <rdfs:comment>Local energy grid that can disconnect from the main transmission grid and operate autonomously using distributed energy resources.</rdfs:comment>
    <rdfs:label>microgrid</rdfs:label>
  </owl:Class>
  <!-- DER-Specific Properties -->
  <!-- Additional DER Properties (common properties now inherited from GenerationAsset) -->
<!-- DER Definitions-->
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-system">
    <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-assets.owl#physical-asset"/>
    <rdfs:comment>DER system can be viewed as composed of two classes of components: physical hardware/firmware components and cyber controller components that manage the physical components</rdfs:comment>
    <rdfs:label>der_system</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-controller">
    <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-device"/>
    <rdfs:comment>Directly manages DER operations, including control of generation, storage, and interconnection behavior. A critical asset at the DER site for both utility and customer-owned systems.</rdfs:comment>
    <rdfs:label>der_controller</rdfs:label>
  </owl:Class>
<owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-user">
  <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-system"/>
  <rdfs:comment>General role representing any individual or organization that interacts with or maintains DER components.</rdfs:comment>
  <rdfs:label>der_user</rdfs:label>
</owl:Class>
<owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-owner">
  <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-user"/>
  <rdfs:comment>An individual or entity that owns DER assets, such as residential, commercial, or utility customers.</rdfs:comment>
  <rdfs:label>der_owner</rdfs:label>
</owl:Class>
<owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-operator">
  <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-user"/>
  <rdfs:comment>Entity responsible for operating and maintaining DER assets on behalf of the owner or a utility.</rdfs:comment>
  <rdfs:label>der_operator</rdfs:label>
</owl:Class>
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-device">
    <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-system"/>
    <rdfs:comment>An individual DER device inside a group of DER that collectively form a system.</rdfs:comment>
    <rdfs:label>der_device</rdfs:label>
  </owl:Class>
<owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#photovoltaic-system">
  <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-device"/>
  <rdfs:comment>DER asset that generates electricity from solar energy using photovoltaic cells.</rdfs:comment>
  <rdfs:label>photovoltaic_system</rdfs:label>
</owl:Class>
<owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#battery-energy-storage-system">
  <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-device"/>
  <rdfs:comment>DER asset used to store and release electricity to balance load and generation.</rdfs:comment>
  <rdfs:label>battery_energy_storage_system</rdfs:label>
</owl:Class>
<owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#smart-inverter">
  <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-device"/>
  <rdfs:comment>Inverter with advanced functionalities like voltage regulation, frequency control, and grid support. Typically complies with IEEE 1547 smart inverter mandates.</rdfs:comment>
  <rdfs:label>smart_inverter</rdfs:label>
</owl:Class>
<owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#smart-meter">
  <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-assets.owl#ot-device"/>
  <rdfs:comment>Standalone smart meter used for residential or commercial energy measurement, monitoring, and billing integration.</rdfs:comment>
  <rdfs:label>smart_meter</rdfs:label>
</owl:Class>
<owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#facility-energy-management-system">
  <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-device"/>
  <rdfs:comment>System that manages energy flows, DER dispatch, and load profiles at a facility level. Can interface with DERMS or local controllers for optimal energy use.</rdfs:comment>
  <rdfs:label>facility_energy_management_system</rdfs:label>
</owl:Class>
<owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-scada">
  <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-device"/>
  <rdfs:comment>Supervisory Control and Data Acquisition system specific to DER operations. Provides remote telemetry, status monitoring, and command dispatch.</rdfs:comment>
  <rdfs:label>der_scada</rdfs:label>
</owl:Class>

<!-- EVSE Components -->
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#electric-vehicle-supply-equipment">
    <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-device"/>
    <rdfs:comment>EVSE provides electric energy for recharging electric vehicles. It includes connectors, power modules, and communication interfaces.  </rdfs:comment>
    <rdfs:label>electric_vehicle_supply_equipment</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#electric-vehicle">
  <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-assets.owl#physical-asset"/>
  <rdfs:comment>Automobile that consumes or provides electricity via an electric vehicle supply equipment (EVSE). Part of vehicle-to-grid (V2G) ecosystems.</rdfs:comment>
  <rdfs:label>electric_vehicle</rdfs:label>
</owl:Class>
<owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#evse-controller">
  <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-device"/>
  <rdfs:comment>Embedded controller within EVSE that manages charging authorization, session scheduling, energy delivery, and communication with utility systems or aggregators.</rdfs:comment>
  <rdfs:label>evse_controller</rdfs:label>
</owl:Class>
<owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#charging-connector">
  <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-device"/>
  <rdfs:comment>Physical interface used to connect an Electric Vehicle to EVSE. Subject to hardware and protocol vulnerabilities.</rdfs:comment>
  <rdfs:label>charging_connector</rdfs:label>
</owl:Class>
<owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#v2g-communication-module">
  <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-device"/>
  <rdfs:comment>Module that enables bi-directional communication between EV, EVSE, and grid systems. Supports V2G protocols such as ISO 15118 or OCPP.</rdfs:comment>
  <rdfs:label>v2g_communication_module</rdfs:label>
</owl:Class>
<owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#energy-meter-evse">
  <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-device"/>
  <rdfs:comment>Meter integrated within EVSE to record energy consumption or injection. Supports billing, analytics, and load tracking.</rdfs:comment>
  <rdfs:label>energy_meter_evse</rdfs:label>
</owl:Class>
<owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#iso-15118-protocol">
  <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-communication-interface"/>
  <rdfs:comment>Standard for vehicle-to-grid communication interface supporting plug-and-charge and secure messaging. Vulnerable to spoofing and man-in-the-middle attacks.</rdfs:comment>
  <rdfs:label>iso_15118_protocol</rdfs:label>
</owl:Class>
<owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#ocpp-protocol">
  <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-communication-interface"/>
  <rdfs:comment>Open Charge Point Protocol. Used by EVSEs to communicate with central management systems. Common in commercial/public charging deployments.</rdfs:comment>
  <rdfs:label>ocpp_protocol</rdfs:label>
</owl:Class>

<!--DER Management-->
<owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#distribution-management-system">
  <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-assets.owl#ot-device"/>
  <rdfs:comment>Utility-side system that models the distribution network, forecasts DER output, and optimizes load flow with DER coordination.</rdfs:comment>
  <rdfs:label>distribution_management_system</rdfs:label>
</owl:Class>
<owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#derms">
  <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#distribution-management-system"/>
  <rdfs:comment>Distributed Energy Resource Management System. Coordinates DER assets across a grid area by enforcing policy, optimization, and real-time response.</rdfs:comment>
  <rdfs:label>derms</rdfs:label>
</owl:Class>

<!--Measurement, Control and Communication-->
<owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-aggregator">
  <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-user"/>
  <rdfs:comment>Entity that aggregates multiple distributed energy resources (DERs) for optimized grid interaction, market bidding, and demand response. May act as an intermediary between prosumers and grid operators.</rdfs:comment>
  <rdfs:label>der_aggregator</rdfs:label>
</owl:Class>
<owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#edge-intelligent-device">
  <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-device"/>
  <rdfs:comment>Edge-level computing device integrated within the DER network, responsible for local AI/ML-based analytics, real-time control decisions, and anomaly detection without reliance on centralized processing.</rdfs:comment>
  <rdfs:label>edge_intelligent_device</rdfs:label>
</owl:Class>
<owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#human-machine-interface">
  <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-device"/>
  <rdfs:comment>Local interface for viewing status and modifying DER controller settings. Identified in Level 1 DER systems as a critical point of interaction and vulnerability.</rdfs:comment>
  <rdfs:label>human_machine_interface</rdfs:label>
</owl:Class>
<owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-firmware">
  <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-device"/>
  <rdfs:comment>Embedded control logic within DER hardware. Often targeted via local communication channels or maintenance ports for unauthorized modification.</rdfs:comment>
  <rdfs:label>der_firmware</rdfs:label>
</owl:Class>
<owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#local-electric-power-system">
  <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-device"/>
  <rdfs:comment>Customer-side electrical environment connecting DERs and loads, including breakers, relays, and transformers. Defines the operating context before utility interconnection.</rdfs:comment>
  <rdfs:label>local_electric_power_system</rdfs:label>
</owl:Class>
<owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#maintenance-port">
  <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-device"/>
  <rdfs:comment>Physical or wireless access interface on DER systems used during commissioning or servicing</rdfs:comment>
  <rdfs:label>maintenance_port</rdfs:label>
</owl:Class>
<owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#sensor-inputs">
  <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-device"/>
  <rdfs:comment>DER-connected sensors that measure parameters such as voltage, current, frequency, irradiance, and temperature. Used in real-time decision-making and control</rdfs:comment>
  <rdfs:label>sensor_inputs</rdfs:label>
</owl:Class>
<owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-network-interface">
  <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-device"/>
  <rdfs:comment>Communication interface (wired or wireless) enabling DER devices to send and receive data from external systems (e.g., DERMS, SCADA)</rdfs:comment>
  <rdfs:label>der_network_interface</rdfs:label>
</owl:Class>
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-communication-interface">
  <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-device"/>
  <rdfs:comment>Interface enabling DER to exchange data and control signals with external systems</rdfs:comment>
  <rdfs:label>der_communication_interface</rdfs:label>
</owl:Class>
<owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#ieee-2030-5">
  <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-communication-interface"/>
  <rdfs:comment>Application-layer protocol for DER control and monitoring, also known as Smart Energy Profile 2 (SEP2). Includes embedded security features.</rdfs:comment>
  <rdfs:label>ieee_2030_5</rdfs:label>
</owl:Class>
<owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#ieee-1815-dnp3">
  <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-communication-interface"/>
  <rdfs:comment>Distributed Network Protocol 3.0 (DNP3) used for SCADA-based DER telemetry and control.</rdfs:comment>
  <rdfs:label>ieee_1815_dnp3</rdfs:label>
</owl:Class>
<owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#sunspec-modbus-tcp">
  <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-communication-interface"/>
  <rdfs:comment>SunSpec-defined Modbus profile over TCP/IP for DER monitoring and control.</rdfs:comment>
  <rdfs:label>sunspec_modbus_tcp</rdfs:label>
</owl:Class>
<owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-components.owl#interconnection-device">
  <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-components.owl#der-device"/>
  <rdfs:comment>Physical switching device (e.g., circuit breaker) that connects or disconnects DER from the utility grid at the Point of Common Coupling. Required for islanding detection, fault clearance, and IEEE 1547 compliance.</rdfs:comment>
  <rdfs:label>interconnection_device</rdfs:label>
</owl:Class>

<!-- Relationships -->
<owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-relationships.owl#controls-device">
  <rdfs:domain rdf:resource="#der-controller"/>
  <rdfs:range rdf:resource="#der-device"/>
  <rdfs:comment>Defines that a controller manages or governs the behavior of a DER device (e.g., inverter, battery).</rdfs:comment>
  <rdfs:label>controls</rdfs:label>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-relationships.owl#communicates-with">
  <rdfs:domain rdf:resource="#der-communication-interface"/>
  <rdfs:range rdf:resource="#der-device"/>
  <rdfs:comment>Indicates that the communication interface is used to interact with a DER device or system.</rdfs:comment>
  <rdfs:label>communicates_with</rdfs:label>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-relationships.owl#monitored-by">
  <rdfs:domain rdf:resource="#der-device"/>
  <rdfs:range rdf:resource="#sensor-inputs"/>
  <rdfs:comment>Describes that a DER device is monitored by specific sensor inputs measuring operational parameters.</rdfs:comment>
  <rdfs:label>monitored_by</rdfs:label>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-relationships.owl#accessed-through">
  <rdfs:domain rdf:resource="#der-device"/>
  <rdfs:range rdf:resource="#human-machine-interface"/>
  <rdfs:comment>Captures access paths to DERs through HMIs or maintenance ports, often exploited in cyber incidents.</rdfs:comment>
  <rdfs:label>accessed_through</rdfs:label>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:about="#owns">
  <rdfs:domain rdf:resource="#der-owner"/>
  <rdfs:range rdf:resource="#der-device"/>
  <rdfs:comment>Indicates that the DER_Owner holds ownership rights over the DER_Device.</rdfs:comment>
  <rdfs:label>owns</rdfs:label>
  <owl:inverseOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-relationships.owl#owned_by"/>
</owl:ObjectProperty>
<owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-relationships.owl#owned-by">
  <rdfs:domain rdf:resource="#der-device"/>
