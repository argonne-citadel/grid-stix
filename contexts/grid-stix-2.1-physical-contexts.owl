<?xml version="1.0"?>
<rdf:RDF xmlns:owl="http://www.w3.org/2002/07/owl#" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xml:base="http://www.anl.gov/sss/grid-stix-2.1-physical-contexts.owl">
  <owl:Ontology rdf:about="http://www.anl.gov/sss/grid-stix-2.1-physical-contexts.owl">
    <owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/observed-data"/>
    <owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/location"/>
    <owl:imports rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-vocab.owl"/>
    <rdfs:label>grid_stix_2_1_physical_contexts</rdfs:label>
    <rdfs:comment>Physical security context definitions for Grid-STIX 2.1</rdfs:comment>
  </owl:Ontology>
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-physical-contexts.owl#PhysicalSecurityContext">
    <rdfs:subClassOf rdf:resource="http://docs.oasis-open.org/ns/cti/stix#ObservedData"/>
    <rdfs:label>physical_security_context</rdfs:label>
    <rdfs:comment>
      Physical Security Context &#x2013; represents physical security events at grid facilities.
    </rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-assets.owl#PhysicalSecurityPerimeter">
    <rdfs:subClassOf rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-assets.owl#SecurityZone"/>
    <rdfs:comment>The physical border surrounding locations where critical cyber assets are housed.</rdfs:comment>
    <rdfs:label>physical_security_perimeter</rdfs:label>
  </owl:Class>
  <owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-physical-contexts.owl#facility_refs">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-physical-contexts.owl#PhysicalSecurityContext"/>
    <rdfs:range rdf:resource="http://docs.oasis-open.org/ns/cti/stix/location"/>
    <rdfs:comment>References to locations/facilities where the security event occurred.</rdfs:comment>
    <rdfs:label>facility_refs</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-physical-contexts.owl#security_event_type">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-physical-contexts.owl#PhysicalSecurityContext"/>
    <rdfs:range rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-vocab.owl#security_event_type_ov"/>
    <rdfs:comment>The type of physical security event that occurred.</rdfs:comment>
    <rdfs:label>security_event_type</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-physical-contexts.owl#security_zone">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-physical-contexts.owl#PhysicalSecurityContext"/>
    <rdfs:range rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-vocab.owl#security_zone_type_ov"/>
    <rdfs:comment>The type of security zone where the event occurred.</rdfs:comment>
    <rdfs:label>security_zone</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-physical-contexts.owl#response_status">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-physical-contexts.owl#PhysicalSecurityContext"/>
    <rdfs:range rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-vocab.owl#response_status_ov"/>
    <rdfs:comment>The current status of the response to the physical security event.</rdfs:comment>
    <rdfs:label>response_status</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-physical-contexts.owl#has_context_physical_security">
    <rdfs:domain rdf:resource="http://docs.oasis-open.org/ns/cti/stix/observed-data"/>
    <rdfs:range rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-physical-contexts.owl#PhysicalSecurityContext"/>
    <rdfs:comment>Links an observed data record to a physical security context.</rdfs:comment>
    <rdfs:label>has_context_physical_security</rdfs:label>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-physical-contexts.owl#severity">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-physical-contexts.owl#PhysicalSecurityContext"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:comment>Severity level of the physical security event on a scale of 1-5.</rdfs:comment>
    <rdfs:label>severity</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-physical-contexts.owl#detection_time">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-physical-contexts.owl#PhysicalSecurityContext"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:comment>Time when the physical security event was detected.</rdfs:comment>
    <rdfs:label>detection_time</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-physical-contexts.owl#description">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-physical-contexts.owl#PhysicalSecurityContext"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment>Textual description of the physical security event.</rdfs:comment>
    <rdfs:label>description</rdfs:label>
  </owl:DatatypeProperty>
</rdf:RDF>
