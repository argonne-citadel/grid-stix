<?xml version="1.0"?>
<rdf:RDF xmlns:owl="http://www.w3.org/2002/07/owl#" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xml:base="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl">
  <owl:Ontology rdf:about="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl">
    <owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/incident"/>
    <owl:imports rdf:resource="http://docs.oasis-open.org/ns/cti/stix/observed-data"/>
    <owl:imports rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-vocab.owl"/>
    <rdfs:label>grid_stix_2_1_operational_contexts</rdfs:label>
    <rdfs:comment>Operational context definitions for Grid-STIX 2.1</rdfs:comment>
  </owl:Ontology>
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#OperationalContext">
    <rdfs:subClassOf rdf:resource="http://docs.oasis-open.org/ns/cti/stix/core#DomainObject"/>
    <rdfs:comment>Base class for operational contexts affecting grid operations.</rdfs:comment>
    <rdfs:label>operational_context</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#EmergencyResponseContext">
    <rdfs:subClassOf rdf:resource="http://docs.oasis-open.org/ns/cti/stix#Incident"/>
    <rdfs:comment>Context describing emergency response actions for the grid.</rdfs:comment>
    <rdfs:label>emergency_response_context</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#GridOperatingConditionContext">
    <rdfs:subClassOf rdf:resource="http://docs.oasis-open.org/ns/cti/stix#ObservedData"/>
    <rdfs:comment>Context representing real-time operating conditions of the grid.</rdfs:comment>
    <rdfs:label>grid_operating_condition_context</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#MaintenanceContext">
    <rdfs:subClassOf rdf:resource="http://docs.oasis-open.org/ns/cti/stix#ObservedData"/>
    <rdfs:comment>Context describing planned or unplanned maintenance activities.</rdfs:comment>
    <rdfs:label>maintenance_context</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#OutageContext">
    <rdfs:subClassOf rdf:resource="http://docs.oasis-open.org/ns/cti/stix#Incident"/>
    <rdfs:comment>Context describing grid outages and service interruptions.</rdfs:comment>
    <rdfs:label>outage_context</rdfs:label>
  </owl:Class>
  <owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#emergency_status">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#EmergencyResponseContext"/>
    <rdfs:range rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-vocab.owl#emergency_status_ov"/>
    <rdfs:comment>Status of emergency response, e.g., deployed, resolved.</rdfs:comment>
    <rdfs:label>emergency_status</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#emergency_type">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#EmergencyResponseContext"/>
    <rdfs:range rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-vocab.owl#emergency_type_ov"/>
    <rdfs:comment>Type of emergency, e.g., fire, security breach.</rdfs:comment>
    <rdfs:label>emergency_type</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#grid_operating_condition">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#GridOperatingConditionContext"/>
    <rdfs:range rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-vocab.owl#grid_operating_condition_ov"/>
    <rdfs:comment>Current operating condition of the grid, e.g., normal, peak load.</rdfs:comment>
    <rdfs:label>grid_operating_condition</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#maintenance_status">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#MaintenanceContext"/>
    <rdfs:range rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-vocab.owl#maintenance_status_ov"/>
    <rdfs:comment>Status of maintenance, e.g., scheduled, in-progress, complete.</rdfs:comment>
    <rdfs:label>maintenance_status</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#maintenance_type">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#MaintenanceContext"/>
    <rdfs:range rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-vocab.owl#maintenance_type_ov"/>
    <rdfs:comment>Type of maintenance, e.g., routine, corrective.</rdfs:comment>
    <rdfs:label>maintenance_type</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#monitoring_level">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#GridOperatingConditionContext"/>
    <rdfs:range rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-vocab.owl#monitoring_level_ov"/>
    <rdfs:comment>Level of system monitoring in effect.</rdfs:comment>
    <rdfs:label>monitoring_level</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#outage_cause">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#OutageContext"/>
    <rdfs:range rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-vocab.owl#outage_cause_ov"/>
    <rdfs:comment>Cause of outage, e.g., equipment failure, weather.</rdfs:comment>
    <rdfs:label>outage_cause</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#outage_type">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#OutageContext"/>
    <rdfs:range rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-vocab.owl#outage_type_ov"/>
    <rdfs:comment>Type of outage, e.g., planned, unplanned.</rdfs:comment>
    <rdfs:label>outage_type</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#response_level">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#EmergencyResponseContext"/>
    <rdfs:range rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-vocab.owl#response_level_ov"/>
    <rdfs:comment>Severity or level of response.</rdfs:comment>
    <rdfs:label>response_level</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#response_team_refs">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#EmergencyResponseContext"/>
    <rdfs:range rdf:resource="http://docs.oasis-open.org/ns/cti/stix/identity"/>
    <rdfs:comment>References to response team identities involved.</rdfs:comment>
    <rdfs:label>response_team_refs</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#voltage_status">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#GridOperatingConditionContext"/>
    <rdfs:range rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-vocab.owl#voltage_status_ov"/>
    <rdfs:comment>Voltage status of the grid.</rdfs:comment>
    <rdfs:label>voltage_status</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#operational_status">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#GridOperatingConditionContext"/>
    <rdfs:range rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-vocab.owl#operational_status_ov"/>
    <rdfs:comment>Overall operational status of the grid component or system.</rdfs:comment>
    <rdfs:label>operational_status</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#hasContext_GridOperatingCondition">
    <rdfs:domain rdf:resource="http://docs.oasis-open.org/ns/cti/stix/observed-data"/>
    <rdfs:range rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#GridOperatingConditionContext"/>
    <rdfs:comment>Links observed data to a real-time operating condition of the grid.</rdfs:comment>
    <rdfs:label>hascontext_gridoperatingcondition</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#hasContext_Maintenance">
    <rdfs:domain rdf:resource="http://docs.oasis-open.org/ns/cti/stix/observed-data"/>
    <rdfs:range rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#MaintenanceContext"/>
    <rdfs:comment>Links observed data to a maintenance context relevant to grid operations.</rdfs:comment>
    <rdfs:label>hascontext_maintenance</rdfs:label>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#actual_restore_time">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#OutageContext"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:comment>Actual time when service was restored.</rdfs:comment>
    <rdfs:label>actual_restore_time</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#current_load">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#GridOperatingConditionContext"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:comment>Current load on the grid in megawatts.</rdfs:comment>
    <rdfs:label>current_load</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#estimated_restore_time">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#OutageContext"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:comment>Estimated time when service will be restored.</rdfs:comment>
    <rdfs:label>estimated_restore_time</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#frequency">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#GridOperatingConditionContext"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:comment>Current frequency of the grid in Hertz.</rdfs:comment>
    <rdfs:label>frequency</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#maintenance_end_time">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#MaintenanceContext"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:comment>End time of maintenance activity.</rdfs:comment>
    <rdfs:label>maintenance_end_time</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#maintenance_start_time">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#MaintenanceContext"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:comment>Start time of maintenance activity.</rdfs:comment>
    <rdfs:label>maintenance_start_time</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#outage_start_time">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#OutageContext"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    <rdfs:comment>Time when the outage began.</rdfs:comment>
    <rdfs:label>outage_start_time</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#emergency_description">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#EmergencyResponseContext"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment>Textual description of the emergency situation.</rdfs:comment>
    <rdfs:label>emergency_description</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#affected_customers">
    <rdfs:domain rdf:resource="http://www.anl.gov/sss/grid-stix-2.1-operational-contexts.owl#OutageContext"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:comment>Number of customers affected by the outage.</rdfs:comment>
    <rdfs:label>affected_customers</rdfs:label>
  </owl:DatatypeProperty>
</rdf:RDF>
